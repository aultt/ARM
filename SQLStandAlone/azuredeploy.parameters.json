{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "VMName":{
      "value": "SQLVM001"
    },  
    "localadminusername": {
      "value": "mylocaladmin"
    },
    "localadminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<YourSubID>/resourceGroups/<YourRG>/providers/Microsoft.KeyVault/vaults/<YourKeyVaultName>"
        },
        "secretName": "localAdmin"
      }
    },
    "existingDomainName": {
      "value": "YourDomain.local"
    },
    "domainUsername": {
      "value": "YourDomain\\user1"
    },
    "domainuserPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<YourSubID>/resourceGroups/<YourRG>/providers/Microsoft.KeyVault/vaults/<YourKeyVaultName>"
        },
        "secretName": "DomainAdmin"
      }
    },
    "existingVirtualNetworkResourceGroup":{
      "value" :"YourResourceGroup"
    },
    "existingVirtualNetworkName": {
      "value": "YourVNet"
    },
    "existingSubnetName": {
      "value": "YourSubnet"
    }
  }
}